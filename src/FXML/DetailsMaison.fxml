            <ToggleGroup fx:id="garantie" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="niveau80" layoutX="338.0" layoutY="414.0" mnemonicParsing="false" onAction="#choice" text="80%" toggleGroup="$garantie">
