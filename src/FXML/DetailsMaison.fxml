      <TextField fx:id="txtRue" layoutX="110.0" layoutY="425.0" prefHeight="25.0" prefWidth="113.0" />
      <Label fx:id="code" layoutX="195.0" layoutY="34.0" opacity="0.0" prefHeight="25.0" prefWidth="32.0" text="Label" />
