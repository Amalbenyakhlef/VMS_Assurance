<AnchorPane id="AnchorPane" prefHeight="540.0" prefWidth="801.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DetailsMaisonController">
   <children>
      <ImageView fitHeight="540.0" fitWidth="830.0" pickOnBounds="true" preserveRatio="true">
