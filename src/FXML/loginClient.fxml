<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="540.0" prefWidth="767.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginClientController">
   <children>
      <ImageView fitHeight="540.0" fitWidth="860.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/login.png" />
         </image>
      </ImageView>
      <TextField fx:id="user" layoutX="571.0" layoutY="200.0" prefHeight="33.0" prefWidth="145.0" promptText="email" style="-fx-background-radius: 10;" />

      <PasswordField fx:id="mdp" layoutX="571.0" layoutY="244.0" prefHeight="33.0" prefWidth="145.0" promptText="mot de passe" style="-fx-background-radius: 10;" />
       <Button fx:id="button" layoutX="594.0" layoutY="321.0" onAction="#login" opacity="0.0" prefHeight="26.0" prefWidth="113.0" text="login" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="50.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btn" layoutX="578.0" layoutY="284.0" mnemonicParsing="false" onAction="#inscrii" opacity="0.0" prefHeight="13.0" prefWidth="44.0" text="Button" />
      <Button fx:id="btnContact" layoutX="610.0" layoutY="39.0" mnemonicParsing="false" onAction="#contact" opacity="0.0" prefHeight="25.0" prefWidth="95.0" text="Button" />
   </children>
</AnchorPane>
